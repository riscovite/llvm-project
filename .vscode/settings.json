{
  "cmake.configureOnOpen": false,
  "cmake.generator": "Ninja",
  "cmake.sourceDirectory": "${workspaceFolder}/llvm",
  "cmake.buildDirectory": "${workspaceFolder}/build/${buildType}/${variant:platform}",
  "cmake.configureSettings": {
    "CMAKE_INSTALL_PREFIX": "${workspaceFolder}/build/${buildType}/dist",
    "LLVM_ENABLE_PROJECTS": ["clang", "lld"],
    "LLVM_ENABLE_RUNTIMES": ["libc", "compiler-rt" /*, "libcxx", "libcxxabi", "libunwind"*/],
    "LLVM_LIBC_FULL_BUILD": "ON",
    "LLVM_RUNTIME_TARGETS": ["riscv64-unknown-riscovite"],
    "LLVM_TARGETS_TO_BUILD": ["RISCV", "X86"],
    "LLVM_RAM_PER_LINK_JOB": "10",
    "LLVM_RAM_PER_COMPILE_JOB": "4",
    "LLVM_RAM_PER_TABLEGEN_JOB": "4",
    "LLVM_OPTIMIZED_TABLEGEN": "ON",
    "LLVM_USE_SPLIT_DWARF": "OFF",
    "LLVM_DEFAULT_TARGET_TRIPLE": "riscv64-unknown-riscovite",
    "LLVM_ENABLE_IDE": "ON",
    "LLVM_STATIC_LINK_CXX_STDLIB": "ON",
    "LLVM_ENABLE_EH": "ON",
    "LLVM_ENABLE_UNWIND_TABLES": "ON",
    "LLVM_ENABLE_RTTI": "ON",
    "LIBUNWIND_ENABLE_SHARED": "OFF",
    "LIBCXX_ENABLE_SHARED": "OFF",
    "LIBCXXABI_ENABLE_SHARED": "OFF",
    "RUNTIMES_riscv64-unknown-riscovite_LIBUNWIND_ENABLE_SHARED": "OFF",
    "RUNTIMES_riscv64-unknown-riscovite_LIBCXX_ENABLE_SHARED": "OFF",
    "RUNTIMES_riscv64-unknown-riscovite_LIBCXXABI_ENABLE_SHARED": "OFF",
    "RUNTIMES_riscv64-unknown-riscovite_COMPILER_RT_BUILD_SANITIZERS": "OFF",
    "RUNTIMES_riscv64-unknown-riscovite_COMPILER_RT_BUILD_XRAY": "OFF",
    "RUNTIMES_riscv64-unknown-riscovite_COMPILER_RT_BUILD_LIBFUZZER": "OFF",
    "RUNTIMES_riscv64-unknown-riscovite_COMPILER_RT_BUILD_BUILTINS": "ON",
    "RUNTIMES_riscv64-unknown-riscovite_CMAKE_SYSROOT": "${workspaceFolder}/build/${buildType}/dist",
  },
  "C_Cpp.dimInactiveRegions": false,
  "git.ignoreLimitWarning": true,
  "files.associations": {
    "algorithm": "cpp",
    "typeinfo": "cpp",
    "cmath": "cpp",
    "complex": "cpp",
    "cstdlib": "cpp",
    "limits": "cpp",
    "new": "cpp",
    "type_traits": "cpp",
    "vector": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "*.tcc": "cpp",
    "bitset": "cpp",
    "cctype": "cpp",
    "chrono": "cpp",
    "cinttypes": "cpp",
    "clocale": "cpp",
    "codecvt": "cpp",
    "condition_variable": "cpp",
    "csignal": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "unordered_map": "cpp",
    "exception": "cpp",
    "filesystem": "cpp",
    "functional": "cpp",
    "iterator": "cpp",
    "map": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "ratio": "cpp",
    "set": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "utility": "cpp",
    "fstream": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "mutex": "cpp",
    "ostream": "cpp",
    "shared_mutex": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "thread": "cpp",
    "variant": "cpp",
    "*.inc": "cpp",
    "*.def": "cpp",
    "*.rh": "cpp",
    "unordered_set": "cpp"
  },
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools"
}
